{
  "name": "keybase-test-vectors",
  "version": "1.0.11",
  "description": "test data shared between different Keybase implementations",
  "main": "js/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/keybase/keybase-test-vectors"
  },
  "author": "keybasE",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "15.8.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "pgp-utils": "0.0.34",
    "prettier": "^1.19.1",
    "purepack": "^1.0.6",
    "regenerator-runtime": "^0.13.3",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "forge-sigchain": "^1.0.8"
  },
  "bin": {
    "imploder": "./bin/imploder"
  },
  "files": [
    "lib/*",
    "bin/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.{json,md,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "modules": "yarn --frozen-lockfile --prefer-offline",
    "modules-prod": "yarn --frozen-lockfile --prefer-offline --prod",
    "dev": "tsc --watch",
    "build": "rm -rf lib/ && tsc",
    "clean": "rm -rf lib/",
    "go": "./bin/generate"
  }
}
